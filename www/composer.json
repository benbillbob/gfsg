{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.6.1",
		"silverstripe/framework": "3.6.1",
		"silverstripe/reports": "3.6.1",
		"silverstripe/siteconfig": "3.6.1",
		"silverstripe-themes/simple": "3.1.*",
		"cwsoft/silverstripe-shortcode": "^2.2",
		"symbiote/silverstripe-memberprofiles": "^2.0",
		"sunnysideup/themes_ssu_flava": "^4.2",
		"rywa/silverstripe-minicart": "^1.0",
		"jonshutt/silverstripe-sortable-gallery-field": "dev-master",
		"silverstripers/silverstripe-timefield": "dev-master"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.6.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
